{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "mise": {
    "enabled": false
  },
  "kubernetes": {
    "fileMatch": [
      "\\.yaml$"
    ]
  },
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "sissbruecker/linkding"
      ],
      "enabled": false
    }
  ]
}
